{
  "name": "trading-bot-ai",
  "version": "1.0.0",
  "description": "Trading bot AI",
  "license": "ISC",
  "author": "Marc Pont",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsup src/index.ts --format esm --clean",
    "start": "NODE_ENV=production node --env-file=.env dist/index.js",
    "check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "script": "tsx --env-file=.env src/scripts/run.ts"
  },
  "dependencies": {
    "@binance/connector-typescript": "^0.3.16",
    "@google/generative-ai": "^0.24.1",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "pm2": "^6.0.14",
    "technicalindicators": "^3.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/pg": "^8.16.0",
    "colors": "^1.4.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "prisma": "^7.4.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=25.0.0"
  }
}
